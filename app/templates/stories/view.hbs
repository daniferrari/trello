<h1>More details about {{model.title}}</h1>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Story {{model.title}}</h3>
  </div>
  <div class="panel-body">
    <p><b>Title: </b> {{model.title}} </p>
    <p><b>Due Date: </b>{{model.dueDate}} </p>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-offset-2 col-sm-10">
  {{#link-to 'story'}}<button type="submit" class="btn btn-default">Back to stories list </button>{{/link-to}}
  </div>
</div>
